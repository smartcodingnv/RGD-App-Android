<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.LabFragment">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <fragment
            android:id="@+id/map"
            class="com.here.android.mpa.mapping.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/lay_pin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#80FFFFFF"
                android:orientation="horizontal"
                android:padding="@dimen/_10sdp">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/btnPin1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:tag="pin1">

                        <ImageView
                            android:id="@+id/ivPin1"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginBottom="2dp"
                            android:src="@drawable/ic_pin"
                            android:visibility="invisible" />

                        <View
                            android:id="@+id/verticleLine1"
                            android:layout_width="1dp"
                            android:layout_height="8dp"
                            android:layout_below="@+id/ivPin1"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="#707070" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/verticleLine1"
                            android:layout_centerHorizontal="true"
                            android:fontFamily="@font/fagono"
                            android:text="5KM"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                    </RelativeLayout>

                    <View
                        android:id="@+id/horizontalLine1"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginStart="14dp"
                        android:layout_marginTop="31dp"
                        android:background="#707070" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="-16dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/btnPin2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:tag="pin2">

                        <ImageView
                            android:id="@+id/ivPin2"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="2dp"
                            android:src="@drawable/ic_pin"
                            android:visibility="invisible" />

                        <View
                            android:id="@+id/verticleLine2"
                            android:layout_width="1dp"
                            android:layout_height="8dp"
                            android:layout_below="@+id/ivPin2"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="#707070" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/verticleLine2"
                            android:layout_centerHorizontal="true"
                            android:fontFamily="@font/fagono"
                            android:text="10KM"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                    </RelativeLayout>

                    <View
                        android:id="@+id/horizontalLine2"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="31dp"
                        android:background="#707070" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="-16dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/btnPin3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:tag="pin3">

                        <ImageView
                            android:id="@+id/ivPin3"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="2dp"
                            android:src="@drawable/ic_pin"
                            android:visibility="invisible" />

                        <View
                            android:id="@+id/verticleLine3"
                            android:layout_width="1dp"
                            android:layout_height="8dp"
                            android:layout_below="@+id/ivPin3"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="#707070" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/verticleLine3"
                            android:layout_centerHorizontal="true"
                            android:fontFamily="@font/fagono"
                            android:text="15KM"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                    </RelativeLayout>

                    <View
                        android:id="@+id/horizontalLine3"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="31dp"
                        android:layout_toRightOf="@+id/layout3"
                        android:background="#707070" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="-18dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/btnPin4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:tag="pin4">

                        <ImageView
                            android:id="@+id/ivPin4"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="2dp"
                            android:src="@drawable/ic_pin"
                            android:visibility="invisible" />

                        <View
                            android:id="@+id/verticleLine4"
                            android:layout_width="1dp"
                            android:layout_height="8dp"
                            android:layout_below="@+id/ivPin4"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="#707070" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/verticleLine4"
                            android:layout_centerHorizontal="true"
                            android:fontFamily="@font/fagono"
                            android:text="20KM"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                    </RelativeLayout>

                    <View
                        android:id="@+id/horizontalLine4"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="31dp"
                        android:layout_marginEnd="14dp"
                        android:background="#707070" />

                    <RelativeLayout
                        android:id="@+id/btnPin5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:tag="pin5">

                        <ImageView
                            android:id="@+id/ivPin5"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="2dp"
                            android:src="@drawable/ic_pin"
                            android:visibility="visible" />

                        <View
                            android:id="@+id/verticleLine5"
                            android:layout_width="1dp"
                            android:layout_height="8dp"
                            android:layout_below="@+id/ivPin5"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="#707070" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/verticleLine5"
                            android:layout_centerHorizontal="true"
                            android:fontFamily="@font/fagono"
                            android:text="All"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                    </RelativeLayout>

                </RelativeLayout>

            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:orientation="vertical"
                    android:padding="@dimen/_10sdp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imgBtnUpArrow"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerInParent="true"
                            android:src="@drawable/ic_arrow_up" />

                        <ImageView
                            android:id="@+id/imgBtnCurrentLocation"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginEnd="5dp"
                            android:src="@drawable/ic_location" />

                        <ImageView
                            android:id="@+id/imgBtnDownloadMap"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_download" />

                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@+id/llPharmacyPracticeList"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scrollbars="vertical">


                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvPharmacyPractice"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:scrollbars="vertical" />

                    </RelativeLayout>

                </LinearLayout>

            </RelativeLayout>
        </LinearLayout>

    </FrameLayout>

</LinearLayout>